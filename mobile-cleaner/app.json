{
  "expo": {
    "name": "CleanProof",
    "slug": "mobile-cleaner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cleaning.mobilecleaner",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is used to verify that you are at the job site when checking in and out.",
        "NSCameraUsageDescription": "The camera is used to take before and after photos as proof of completed cleaning jobs.",
        "NSPhotoLibraryAddUsageDescription": "Photos may be saved to your photo library when you choose to share job reports.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cleaning.mobilecleaner"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "8982ecae-65f1-4d95-86b1-5e5f06c047be"
      }
    },
    "owner": "pa6ha_007"
  }
}
